<?xml version='1.0' encoding='UTF-8'?>
<feed xmlns='http://www.w3.org/2005/Atom' xmlns:apps='http://schemas.google.com/apps/2006'>
  <title>Service Labels Filters</title>

  <entry>
    <category term='filter'></category>
    <title>LinkedIn</title>
    <apps:property name='from' value='@linkedin.com OR @e.linkedin.com'/>
    <apps:property name='label' value='Services/LinkedIn'/>
    <apps:property name='shouldNeverSpam' value='true'/>
  </entry>

  <entry>
    <category term='filter'></category>
    <title>GitHub</title>
    <apps:property name='from' value='@github.com'/>
    <apps:property name='label' value='Services/GitHub'/>
    <apps:property name='shouldNeverSpam' value='true'/>
  </entry>

  <entry>
    <category term='filter'></category>
    <title>Keeper</title>
    <apps:property name='from' value='@keepersecurity.com'/>
    <apps:property name='label' value='Services/Keeper'/>
    <apps:property name='shouldNeverSpam' value='true'/>
  </entry>

  <entry>
    <category term='filter'></category>
    <title>X (Twitter)</title>
    <apps:property name='from' value='@x.com OR @twitter.com'/>
    <apps:property name='label' value='Services/X'/>
    <apps:property name='shouldNeverSpam' value='true'/>
  </entry>

  <entry>
    <category term='filter'></category>
    <title>Google</title>
    <apps:property name='from' value='@google.com OR @accounts.google.com'/>
    <apps:property name='label' value='Services/Google'/>
    <apps:property name='shouldArchive' value='true'/>
    <apps:property name='shouldNeverSpam' value='true'/>
  </entry>

</feed>
